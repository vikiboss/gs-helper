{
  "name": "gs-helper",
  "productName": "GenshinHelper",
  "private": true,
  "version": "1.2.4",
  "description": "PC 端小工具，支持原神签到、祈愿分析、查看便签状态和游戏详细数据等。基于 Electron 和 React。",
  "main": ".webpack/main",
  "prettier": "@vikiboss/prettier-config",
  "scripts": {
    "dev": "electron-forge start",
    "dev:watch": "electron-forge start --inspect-electron",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "lint": "pnpm run prettier && pnpm run eslint",
    "lint:fix": "pnpm run prettier:fix && pnpm run eslint:fix",
    "prettier": "prettier ./**/*.{sass,less,css,html,md,json}",
    "prettier:fix": "prettier --write ./**/*.{sass,less,css,html,md,json}",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "eslint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ."
  },
  "repository": "vikiboss/gs-helper",
  "homepage": "https://github.com/vikiboss/gs-helper#readme",
  "author": "Viki <hi@viki.moe> (https://github.com/vikiboss)",
  "license": "GPL-3.0",
  "config": {
    "forge": "./forge.config.js"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.5",
    "@electron-forge/maker-zip": "^6.0.5",
    "@electron-forge/plugin-webpack": "^6.0.5",
    "@types/dom-to-image": "^2.6.4",
    "@types/fs-extra": "^9.0.13",
    "@types/json-bigint": "^1.0.1",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/uuid": "^8.3.4",
    "@vercel/webpack-asset-relocator-loader": "1.7.0",
    "@vikiboss/prettier-config": "^0.2.2",
    "conf": "^10.2.0",
    "css-loader": "^6.7.3",
    "electron": "^21.4.1",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-viki-react": "^0.1.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "fork-ts-checker-webpack-plugin": "^6.5.2",
    "json-schema-typed": "^8.0.1",
    "less": "^4.1.3",
    "less-loader": "^10.2.0",
    "node-loader": "^2.0.0",
    "prettier": "^2.8.4",
    "prop-types": "^15.8.1",
    "react-router": "^6.8.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0"
  },
  "dependencies": {
    "@nivo/bar": "^0.80.0",
    "@nivo/calendar": "^0.80.0",
    "@nivo/core": "^0.80.0",
    "@nivo/pie": "^0.80.0",
    "@nivo/tooltip": "0.80.0",
    "axios": "^1.3.2",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "dom-to-image": "^2.6.0",
    "electron-store": "^8.1.0",
    "fs-extra": "^11.1.0",
    "json-bigint": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-router-dom": "^6.8.1",
    "uuid": "^8.3.2"
  }
}
