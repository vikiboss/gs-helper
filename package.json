{
  "name": "gs-helper",
  "productName": "GenshinHelper",
  "packageManager": "pnpm@7.29.0",
  "private": true,
  "version": "1.2.4",
  "description": "PC 端小工具，支持原神签到、祈愿分析、查看便签状态和游戏详细数据等。基于 Electron 和 React。",
  "main": ".webpack/main",
  "scripts": {
    "dev": "electron-forge start",
    "dev:watch": "electron-forge start --inspect-electron",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "lint": "pnpm run prettier && pnpm run eslint",
    "lint:fix": "pnpm run prettier:fix && pnpm run eslint:fix",
    "prettier": "prettier ./**/*.{less,html,md,json}",
    "prettier:fix": "prettier --write ./**/*.{less,html,md,json}",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "eslint:fix": "eslint --ext .js,.jsx,.ts,.tsx . --fix"
  },
  "repository": "vikiboss/gs-helper",
  "homepage": "https://github.com/vikiboss/gs-helper#readme",
  "author": "Viki <hi@viki.moe> (https://github.com/vikiboss)",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/dom-to-image": "^2.6.4",
    "@types/fs-extra": "^9.0.13",
    "@types/json-bigint": "^1.0.1",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^8.3.4",
    "@vmoe/eslint-config": "^0.3.1",
    "@vmoe/prettier-config": "^1.0.0",
    "autoprefixer": "^10.4.14",
    "electron": "^24.1.1",
    "eslint": "^8.38.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.4",
    "vite": "^4.2.1"
  },
  "dependencies": {
    "@nivo/bar": "^0.80.0",
    "@nivo/calendar": "^0.80.0",
    "@nivo/core": "^0.80.0",
    "@nivo/pie": "^0.80.0",
    "@nivo/tooltip": "0.80.0",
    "axios": "^1.3.5",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "dom-to-image": "^2.6.0",
    "electron-store": "^8.1.0",
    "fs-extra": "^11.1.1",
    "json-bigint": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.10.0",
    "uuid": "^8.3.2"
  }
}
